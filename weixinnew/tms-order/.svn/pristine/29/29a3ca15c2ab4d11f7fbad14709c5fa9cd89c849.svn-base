*{
    margin:0;
    padding:0;
    -webkit-tap-highlight-color:transparent;
}
html,body{
    background: #f2f2f2;
    font-size: 0.16rem;
}
input{
    outline: none;
    height:0.3rem;
    border:none;
}
nav{
    width:100%;
    height:0.44rem;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: #000000;
    color: #ffffff;
}
nav div:first-of-type{
    margin:0 0 0 0.2rem;
}
main{
    width:100%;
    height:auto;
    display: flex;
    flex-direction: column;
}
main ul{
    background: #ffffff;
    margin:0.2rem 0 0.15rem 0;
    border-bottom:1px solid #000000;

}
main ul li{
    display: flex;
    height:0.55rem;
    justify-content: space-between;
    align-items: center;
    padding:0 0 0 0.15rem;
    line-height: 0.35rem;
}
main ul li span:first-child{
    width:37%;
    line-height: 100%;
}
main ul li div{
    border-bottom: 1px solid #ccc;
    line-height: 100%;
    width:100%;
    height:100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
main ul li div span:last-child{
    width:0.7rem;
    height:0.3rem;
    display: inline-block;
    border-radius: 0.05rem;
    text-align: center;
    line-height: 0.3rem;

}
.lkj{
    width: 1.5rem;
}
.fasf span,.leftPro li,.shouleftPro li,.shourightCity li,.rightCity li{
    cursor: pointer;
}
#fasas{

    margin-right: 0.05rem;
    background: #009dd9;
    color: #ffffff;
    width:1rem;
    padding:0.01rem;
}
.tou{
    height:0.8rem;
    background: #ffffff;
    border:1px solid #000000;
    border-left:none;
    border-right:none;
    padding:0.1rem 0 0 0;
}
.tou div{
    display: flex;
}
.tou div:first-of-type{
    line-height: 0.3rem;
    padding:0 0 0.2rem 0.1rem;
}
.tou div:last-of-type{
    justify-content: space-around;
}
main ul li:nth-child(6) div{
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
textarea{
    resize: none;
    height:0.6rem;
    width:100%;
    outline: none;
    border:none;
}
footer{
    width:100%;
    position: relative;
    padding:0.1rem 0 0.1rem 0;
}
footer span,#con span{
    border-radius: 5px;
    background: #009dd9;
    color: #ffffff;
    width:80%;
    margin:0 0 0 10%;
    text-align: center;
    line-height: 0.4rem;
    cursor: pointer;
    display: inline-block;
}
#con {
    width: 100%;
    padding: 0.5rem 0 0.1rem 0;
    position: inherit;
    background: inherit;
}
.wenTan div:last-of-type p{
    line-height: 0.3rem;
}
.fda{
    width:100%;
    height:200%;
    position: absolute;
    top:0;
    z-index: 99;
    display: none;
}
.fda div:first-of-type{
    width:100%;
    height:100%;
    opacity: 0.5;
    background: #000000;
}
.fda div:last-of-type{
    width:100%;
    height: auto;
    background: #ffffff;
    opacity: 1!important;
    position: fixed;
    z-index: 999;
    bottom:0;
}
.fda div p{
    text-align: center;
}
.rong{
    display:flex;
    flex-direction:column;
    width:100%;
    height: auto;
    padding:0.1rem 0 0 0;
    background: #ffffff;
    opacity: 1!important;
    position: absolute;
    z-index: 999;
    bottom:0;
}
.rong div:nth-child(2){
    display: flex;
    justify-content: left;
    flex-wrap: wrap;

}
.rong div:nth-child(2) span{
    display: inline-block;
    border:1px solid #000000;
    width: 1rem;
    line-height: 0.3rem;
    margin:0.2rem 0 0 0.2rem;
    text-align: center;

}
.nextNum{
    display: flex;
    justify-content: center;
    padding-top:0.1rem;
}
.nextNum span{
    display: inline-block;
    width: 1rem;
    line-height: 0.5rem;
    text-align: center;
    background-color: #009dd9;
    color:#ffffff;
    
}
.rong div:first-of-type{
    background-color: #ffffff;
    color: #000000;
    opacity: 1!important;
}
.wenle{
    padding: 0 0 0.2rem 0;
}
.wenle span{
    line-height: 0.5rem!important;
    font-size: 0.16rem;
}

.wejiNum span{

    margin-left: 0.2rem;
}
.wejiNum span:not(:first-of-type){
    margin-left: 0.3rem;
}
.deep{
    display: none;
    width:100%;
    height: auto;
    padding:0.1rem 0 0 0;
    background: #ffffff;
    opacity: 1!important;
    position: absolute;
    z-index: 999;
    bottom:0;
}
.deep section{
    margin-left: 0.2rem;
}
.deep section span{
    font-size: 0.16rem;
}
.deep section span:last-of-type{
    margin-left: 0.6rem;
}
.deep div:first-of-type{
     background-color: #ffffff;
     opacity: 1!important;
     
}
.deep div:first-of-type ul{

   list-style: none;
}
.deep div:first-of-type ul li{
    margin-top: 0.1rem;
    display: flex;
    justify-content: center;
    align-items: center;
}
.deep div:first-of-type ul li span{
    color: #000000;
    margin-right: 0.1rem;
}
.rongque {
    display: flex;
    justify-content: center;
    position: relative!important;
    padding-top:0.1rem;
}
.rongque span{
    display: inline-block;
    width: 1rem;
    line-height: 0.5rem;
    text-align: center;
    background-color: #009dd9;
    color:#ffffff;
 
}
.dujiTan{
    display: flex;
    flex-direction: column;
    display: none;
    width: 100%;
    height: auto;
    position: absolute;
    z-index: 999;
    bottom: 0;
}
.afsda{
    position: absolute;
    bottom: 0;
    width: 100%;
    height: auto;
    padding: 0.1rem 0 0 0;
}
.afsda div:first-of-type{
     background-color:#ffffff;
     opacity: 1!important;
}
.wenlei{
    background-color:#ffffff;
    color: #000000;
    display: flex;
    justify-content: space-around;
}
.wenlei span{
    width: 1rem;
    line-height: 0.5rem;
    text-align: center;
    border:1px solid #000000;
}
.spanActive{
    background: #009dd9;
    color: #ffffff;
    border:none;
}

.sel{
    -webkit-appearance: none;  
    -webkit-tap-highlight-color: #fff;  
    outline: 0; 
    width: 2rem;
    margin-right: 0.05rem;
    border: none;
    padding: 0 0 0 0.1rem;
}
.faCom,.shouCom{
    border:none;color:#757575;width:43%;
}

.cit p,.shouCit p{
    color: #0093ff;
    height:0.5rem;
    text-align: right!important;
    padding-right: 0.1rem;
    line-height: 0.5rem;
    border-bottom: 1px solid #dedede;
}
.cit .leftPro,.shouCit .shouleftPro{
    width: 40%;
    float:left;
    /* height:; */
    background: #f2f2f2;
    /* overflow: scroll!important; */
}
.cit .leftPro li,.rightCity li,.shourightCity li,.shouleftPro li{
    text-align: center;
    height: 0.5rem;
    list-style-type: none;
    line-height: 0.5rem;
}
.rightCity,.shourightCity{
    width: 55%;
    padding:0 0 0 0.1rem;
    float: right;
}
.rightCity li,.shourightCity li{
    border-bottom:1px solid #dedede;
}

.active{
    border-left:3px solid red;
    background: #ffffff;
}
